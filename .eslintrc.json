{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    // "plugin:vue/recommended",
    "@nuxtjs/eslint-config-typescript",
    "plugin:nuxt/recommended"
    // "prettier"
  ],
  "plugins": [],
  // add your custom rules here
  "rules": {
    "semi": [
      "error",
      "always"
    ],
    "space-before-function-paren": [
      "error",
      "never"
    ],
    "comma-dangle": [
      "error",
      "never"
    ]
  }
}
